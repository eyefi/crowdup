{
  "preferGlobal": true,
  "main": "index",
  "name": "crowdup",
  "description": "A simple CLI tool to update crowdin translations for web applications",
  "version": "0.0.1",
  "author": "Daniel Bohannon <tkdan235@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:eyefi/crowdup.git"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "directories": {
    "bin": "bin",
    "lib": "lib"
  },
  "keywords": [
    "cli",
    "crowdin",
    "translations",
    "update"
  ],
  "dependencies": {
    "async": "^0.9.0",
    "coffee-script": "^1.8.0",
    "colors": "^0.6.2",
    "commander": "git://github.com/zhiyelee/commander.js.git#buginfo",
    "fs-extra": "^0.11.1",
    "lodash": "^2.4.1",
    "node-dir": "^0.1.6",
    "prompt": "^0.2.14"
  },
  "coffeelintConfig": {
    "max_line_length": {
      "name": "max_line_length",
      "value": 120,
      "level": "error",
      "limitComments": true
    }
  }
}
